{
	"presets": [
		"react",
		[
			"env",
			{
				"targets": {
					"browsers": ["last 4 versions"]
				}
			}
		]
	],
	"plugins": [
		[
			"transform-react-jsx",
			{
				"pragma": "Nerv.createElement"
			}
		],
		"transform-object-rest-spread"
	],
	"env": {
		"development": {
			"retainLines": true
		}
	}
}
